{
  "name": "@gusto/embedded-react-sdk",
  "version": "0.0.1",
  "packageManager": "bun@1.1.30",
  "module": "./dist/index.js",
  "type": "module",
  "exports": "./dist/index.js",
  "files": [
    "dist",
    "./README.md",
    "./CHANGELOG.md"
  ],
  "scripts": {
    "build": "bun build:clean && bun openapi:generate && tsc -b --pretty",
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "dev": "tsc -b --watch",
    "doc": "typedoc",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "i18n:generate": "i18next-resources-for-ts interface -i ./src/i18n/en -o ./src/types/i18nresources.d.ts",
    "openapi": "bun openapi:download && bun openapi:generate",
    "openapi:download": "bun build/getTargetSpecFromReadme.js",
    "openapi:generate": "bun build/generateOpenApiFiles.js",
    "prepublishOnly": "bun run build",
    "test": "jest"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.59.15",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/bun": "latest",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "i18next-resources-for-ts": "^1.5.0",
    "jest": "^29.7.0",
    "msw": "2.4.3",
    "openapi-typescript": "^7.4.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.10"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.9.0",
    "@internationalized/date": "^3.5.6",
    "@internationalized/number": "^3.5.4",
    "@tanstack/react-query": "^5.59.15",
    "dompurify": "^3.1.7",
    "i18next": "^23.11.2",
    "openapi-fetch": "^0.12.2",
    "react": "^18.3.1",
    "react-aria": "^3.35.1",
    "react-aria-components": "^1.4.1",
    "react-error-boundary": "^4.1.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.3",
    "react-robot": "^0.3.0",
    "robot3": "^0.4.1",
    "valibot": "^0.42.1"
  }
}
